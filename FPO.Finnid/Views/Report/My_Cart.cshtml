
@{
    ViewData["Title"] = "My_Cart";
    Layout = "_LayoutAdmin";
}

<div class="container-fluid">
    <div class="row">
        <div class="col-lg-12 grid-margin stretch-card">
            <div class="card">
                <div class="card-body">
                    <h4 class="card-title">My Cart</h4>
                    
                    <table class="table table-bordered">
                        <thead>
                            <tr class="t_head">
                                <th> Sr. No </th>
                                <th> Plan </th>
                                <th> Price </th>
                                <th> GST </th>
                                <th> Plan Type </th>
                                <th> Entry On </th>
                            </tr>
                        </thead>
                        <tbody id="My_cart_table">

                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
@section Scripts {
    <script src="~/ProjectJS/Report/My_Cart.js" asp-append-version="true"></script>
}
